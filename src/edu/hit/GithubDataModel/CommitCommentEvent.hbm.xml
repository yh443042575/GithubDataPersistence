<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="edu.hit.GithubDataModel">
	
	<class name="CommitCommentEvent" >
		<id name="commitComment_id"></id>
		<property name="commit_id"></property>
		<property name="actor"></property>
		<property name="html_url"></property>
		<property name="create_time"></property>
		<property name="body"></property>
	</class>

</hibernate-mapping>